    <url>/2024/04/11/2024%E9%99%88%E7%90%B3%E6%80%BB%E9%80%89jz/</url>
    
    <content type="html"><![CDATA[<div id="myChart" style="width: 900px;height:800px; margin-right: auto;"></div><script src="https://code.highcharts.com/highcharts.js"></script><script src="https://code.highcharts.com/highcharts-more.js"></script><script src="https://cdnjs.cloudflare.com/ajax/libs/PapaParse/5.3.0/papaparse.min.js"></script><script>console.log(window.location.origin)function getRandomInt(max) {    return Math.floor(Math.random() * Math.floor(max));}function getRandomColor() {    var letters = '0123456789ABCDEF';    var color = '#';    for (var i = 0; i < 6; i++) {        color += letters[Math.floor(Math.random() * 16)];    }    return color;}fetch('http://101.43.229.240/home/lighthouse/Documents/2024414pk.csv')    .then(response => response.text())    .then(csvString => {        const data = Papa.parse(csvString, { header: true, dynamicTyping: true }).data;        Highcharts.chart('myChart', {    chart: {        type: 'bubble',        plotBorderWidth: 1,        zoomType: 'xy'    },    title: {        text: '2024总选pk个人jz'    },    xAxis: {        visible: false    },    yAxis: {        visible: false    },    tooltip: {        useHTML: true,        headerFormat: '',        pointFormat: '<b>{point.name}: {point.z}</b>',        footerFormat: '',        followPointer: true    },    series: [{        symbolSize: function (data) {                return Math.sqrt(data[2]) * 200;            },        type: 'scatter',        itemStyle: {            color: function() {                return '#' + Math.floor(Math.random()*16777215).toString(16);            }        },        label: {            show: true,            formatter: function (params) {                return params.data.name;            },            position: 'inside'        },        emphasis: {            label: {                show: true,                formatter: function (params) {                    if (Math.sqrt(params.data.value[2]) * 10 < threshold) {                        return '';                    } else {                        return params.data.name + ': ' + params.data.value[2];                    }                }            }        },        name: '金额',        data: data.map(row=> ({                    x: getRandomInt(100),                    y: getRandomInt(100),                    z: row['金额'] * 100,                    name: row['ID'],                    color: getRandomColor(),                            }))            }]});    });</script>]]></content>
    
    
    
